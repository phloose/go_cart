    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug cartogram with -g and -a flags",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/cartogram",
            "args": [
                "-g",
                "../VG250_LAN_ohne_gewaesser_mit_flaeche_processedmap.json",
                "-a",
                "../VG250_LAN_ohne_gewaesser_mit_flaeche_data.csv"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "/usr/bin/gdb"
        },
        {
            "name": "Debug core dump (gdb)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/cartogram",
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "coreDumpPath": "${workspaceFolder}/core"
        },
        {
            "name": "Python: Aktuelle Datei",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "-d",
                "/home/phloose/workspace/EACM/corona_bundesl√§nder.geojson",
                "-n",
                "LAN_ew_GEN",
                "-v",
                "Fallzahl",
            ]
        }
    ]
}
